<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yuuya.ainudesignapp" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".TitleActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MakeActivity"
            android:label="@string/title_activity_make"
            android:parentActivityName=".TitleActivity">
        </activity>
        <activity
            android:name=".ShowActivity"
            android:label="@string/title_activity_show"
            android:parentActivityName=".TitleActivity">
        </activity>
        <activity
            android:name=".ShereActivity"
            android:label="@string/title_activity_shere"
            android:parentActivityName=".TitleActivity">
        </activity>
        <activity
            android:name=".OutputActivity"
            android:label="@string/title_activity_output"
            android:parentActivityName=".TitleActivity">
        </activity>
    </application>

</manifest>
